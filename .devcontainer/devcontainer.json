{
	"name": "Agencia",
	"build": {
		"dockerfile": "Dockerfile", 
		"context": "..", 
		"cacheFrom": [
			"type=registry,ref=ghcr.io/austinmwhaley/agencia:latest",
			"type=local,src=../docker-cache"
		]},
 
	"workspaceMount": "source=${localWorkspaceFolder},target=/agencia,type=bind",
	"workspaceFolder": "/agencia", 

	"customizations": {
		"vscode": {
		"extensions": ["ms-azuretools.vscode-docker", "github.vscode-github-actions", "geddski.macros", "wraith13.open-in-github-desktop"]
		}
	}
}